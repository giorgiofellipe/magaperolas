{
  "name": "angular-github-pages",
  "description": "Angular Github Pages",
  "homepage": "https://github.com/jbeurel/angular-github-pages",
  "repository": {
    "type": "git",
    "url": "git@github.com:jbeurel/angular-github-pages.git"
  },
  "engines": {
    "node": "0.10.x"
  },
  "scripts": {
    "postinstall": "node_modules/.bin/bower install | xargs echo && gulp build",
    "test": "karma start test/karma.coffee --single-run & webdriver-manager start & protractor --require coffee-script/register test/protractor.conf.coffee",
    "test:unit": "node_modules/.bin/karma start test/karma.coffee &",
    "test:functional": "webdriver-manager start &  protractor --require coffee-script/register test/protractor.conf.coffee",
    "watch": "node_modules/.bin/gulp watch",
    "deploy-github": "node_modules/.bin/gulp deploy",
    "deploy-parse": "parse deploy"
  },
  "dependencies": {
    "bower": "1.3.x",
    "coffee-script": "1.7.x",
    "connect-history-api-fallback": "0.0.4",
    "gulp": "3.6.x",
    "gulp-angular-templatecache": "1.1.x",
    "gulp-bump": "0.1.x",
    "gulp-changed": "0.2.x",
    "gulp-clean": "0.2.x",
    "gulp-coffee": "1.4.x",
    "gulp-concat": "2.1.x",
    "gulp-connect": "2.0.x",
    "gulp-filesize": "0.0.x",
    "gulp-git": "0.3.x",
    "gulp-jade": "0.4.x",
    "gulp-less": "1.2.x",
    "gulp-manifest": "0.0.x",
    "gulp-ngmin": "0.3.x",
    "gulp-plug": "0.2.x",
    "gulp-rename": "1.2.x",
    "gulp-replace": "0.2.x",
    "gulp-rev-all": "0.2.x",
    "gulp-uglify": "0.2.x",
    "gulp-util": "2.2.x",
    "jade": "1.3.x",
    "lodash": "2.4.x",
    "run-sequence": "0.3.x"
  },
  "devDependencies": {
    "chai": "1.8.x",
    "chai-as-promised": "4.1.x",
    "cucumber": "*",
    "gulp-debug": "^0.3.0",
    "gulp-gh-pages": "^0.3.3",
    "gulp-minify-css": "^0.3.6",
    "gulp-minify-html": "^0.1.4",
    "gulp-webserver": "^0.3.4",
    "karma": "0.12.x",
    "karma-chai": "0.1.x",
    "karma-cli": "0.0.4",
    "karma-coffee-preprocessor": "0.1.x",
    "karma-coverage": "0.2.x",
    "karma-mocha": "0.1.x",
    "karma-phantomjs-launcher": "0.1.x",
    "karma-sinon-chai": "0.1.x",
    "phantomjs": "1.9.x",
    "protractor": "0.22.x",
    "should": "2.1.x",
    "sinon": "1.10.x",
    "sinon-chai": "2.5.x",
    "webdriver-manager": "0.0.4"
  },
  "version": "0.0.0",
  "main": "gulpfile.js",
  "directories": {
    "test": "test"
  }
}
